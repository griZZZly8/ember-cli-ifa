{
  "name": "ember-cli-ifa",
  "version": "0.7.0",
  "description": "Inject fingerprinted assetMap.json file into your app and provide initializer, service, and helper to dynamically reference fingerprinted assets.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment production",
    "start": "ember server",
    "test": "ember try:each",
    "test-no-fastboot": "FASTBOOT_DISABLED=true ember test",
    "lint:js": "eslint ./*.js app config lib server tests"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/RuslanZavacky/ember-cli-ifa.git"
  },
  "engines": {
    "node": ">= 6"
  },
  "author": {
    "name": "Ruslan Zavacky",
    "email": "ruslan.zavacky@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.6.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "^3.0.0",
    "ember-cli-app-version": "^3.1.3",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-fastboot": "^1.1.3",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-qunit": "^4.3.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-fastboot-test-helpers": "^0.2.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.5.0",
    "ember-source": "^3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.3",
    "eslint-plugin-node": "^6.0.0",
    "fastboot": "^1.1.3",
    "fastboot-app-server": "^1.1.0",
    "jsdom": "^11.6.2",
    "loader.js": "^4.6.0"
  },
  "keywords": [
    "ember-addon",
    "fingerprinting",
    "fingerprint",
    "assetMap",
    "asset-map"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.11.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
